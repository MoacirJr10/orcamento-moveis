{
  "name": "orcamento-frontend",
  "version": "1.0.0",
  "description": "Sistema de Orçamentos de Móveis",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200 --open --disable-host-check",
    "dev": "ng serve --port 4200 --open --configuration development",
    "build": "ng build --configuration production",
    "build:ssr": "ng build && ng run orcamento-frontend:server:production",
    "serve:ssr": "node dist/orcamento-frontend/server/server.mjs",
    "prerender": "ng run orcamento-frontend:prerender",
    "test": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "test:watch": "ng test",
    "lint": "ng lint --fix",
    "e2e": "ng e2e",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/orcamento-frontend/browser/stats.json",
    "prepare": "husky install",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "security-check": "npm audit --production",
    "update": "ng update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.1",
    "@angular/common": "^19.2.1",
    "@angular/compiler": "^19.2.1",
    "@angular/core": "^19.2.1",
    "@angular/forms": "^19.2.1",
    "@angular/material": "^19.2.1",
    "@angular/cdk": "^19.2.1",
    "@angular/platform-browser": "^19.2.1",
    "@angular/platform-browser-dynamic": "^19.2.1",
    "@angular/platform-server": "^19.2.1",
    "@angular/router": "^19.2.1",
    "@angular/ssr": "^19.2.1",
    "compression": "^1.8.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.3",
    "helmet": "^8.1.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "winston": "^3.12.0",
    "winston-daily-rotate-file": "^4.7.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.1",
    "@angular/cli": "^19.2.1",
    "@angular/compiler-cli": "^19.2.1",
    "@types/compression": "^1.7.6",
    "@types/express": "^4.17.21",
    "@types/jasmine": "~5.1.5",
    "@types/node": "^20.11.19",
    "eslint": "^8.56.0",
    "eslint-plugin-angular": "^4.6.0",
    "husky": "^9.0.11",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.5",
    "puppeteer": "^21.9.0",
    "typescript": "~5.3.3",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "browserslist": {
    "production": [
      "last 2 Chrome versions",
      "last 2 Firefox versions",
      "last 2 Edge versions",
      "last 2 Safari versions",
      "last 2 iOS versions"
    ],
    "development": [
      "last 1 Chrome version",
      "last 1 Firefox version",
      "last 1 Edge version",
      "last 1 Safari version"
    ]
  },
  "engines": {
    "node": ">=18.12.0",
    "npm": ">=9.0.0",
    "pnpm": ">=8.0.0"
  },
  "lint-staged": {
    "src/**/*.{ts,html,scss,json}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "resolutions": {
    "@angular/core": "^19.2.1",
    "rxjs": "~7.8.1"
  }
}
